{
  "kind": "collectionType",
  "collectionName": "categorias",
  "info": {
    "singularName": "categoria",
    "pluralName": "categorias",
    "displayName": "Categoria"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "nombre": {
      "type": "string",
      "required": true,
      "unique": false,
      "minLength": 3,
      "maxLength": 20
    },
    "img": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": true
    },
    "detalle": {
      "type": "text",
      "maxLength": 150
    },
    "comercio": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::comercio.comercio",
      "inversedBy": "categorias"
    },
    "sub_categorias": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::sub-categoria.sub-categoria",
      "mappedBy": "categoria"
    }
  }
}
